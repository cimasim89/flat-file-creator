    "flat-file",
    "positional-dataset"
  ],
  "author": "cimasim89",
  "email": "cimasim89@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/cimasim89/flat-file-creator#readme",
  "repository": "github:cimasim89/flat-file-creator",
  "bugs": {
    "url": "https://github.com/cimasim89/flat-file-creator/issues"
  },
  "dependencies": {
    "@types/lodash": "^4.14.171",
    "husky": "^6.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/jest": "26.0.23",
    "@types/node": "16.4.0",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-eslint-plugin": "3.0.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "5.0.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "ts-jest": "26.5.6",
    "ttypescript": "1.5.12",
    "typescript": "4.2.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "always"
  },
  "engines": {
    "node": ">10.0.4"
  },
  "jest": {
    "globalSetup": "./jest-global-setup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "roots": [
      "src/"
    ]
  }
