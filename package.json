{
  "name": "flat-file-creator",
  "version": "2.0.0",
  "description": "Generate a flat positional text file of your dataset",
  "main": "./dist/flat-file-creator.js",
  "types": "./dist/flat-file-creator.d.ts",
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "lint": "eslint --cache --format codeframe src/*.ts",
    "lint:fix": "eslint --fix --cache --format codeframe src/*.ts",
    "typecheck": "yarn ttsc --noEmit",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "positional",
    "file-generator",
    "flat-file",
    "positional-dataset"
  ],
  "author": "cimasim89",
  "email": "cimasim89@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/cimasim89/flat-file-creator#readme",
  "repository": "github:cimasim89/flat-file-creator",
  "bugs": {
    "url": "https://github.com/cimasim89/flat-file-creator/issues"
  },
  "dependencies": {
    "@types/lodash": "^4.14.155",
    "lodash": "^4.17.15",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "eslint": "7.3.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-eslint-plugin": "2.2.2",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.16.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "ts-jest": "26.1.3",
    "ttypescript": "^1.5.10",
    "typescript": "3.9.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "always"
  },
  "engines": {
    "node": ">10.0.4"
  },
  "jest": {
    "globalSetup": "./jest-global-setup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
