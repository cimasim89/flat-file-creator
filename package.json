{
  "name": "flat-file-creator",
  "version": "2.2.0",
  "description": "Generate a flat positional text file of your dataset",
  "main": "./dist/flat-file-creator.js",
  "types": "./dist/flat-file-creator.d.ts",
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "lint": "eslint --cache --format codeframe src/*.ts",
    "lint:fix": "eslint --fix --cache --format codeframe src/*.ts",
    "typecheck": "yarn ttsc --noEmit",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "positional",
    "file-generator",
    "flat-file",
    "positional-dataset"
  ],
  "author": "cimasim89",
  "email": "cimasim89@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/cimasim89/flat-file-creator#readme",
  "repository": "github:cimasim89/flat-file-creator",
  "bugs": {
    "url": "https://github.com/cimasim89/flat-file-creator/issues"
  },
  "dependencies": {
    "@types/lodash": "^4.14.155",
    "lodash": "^4.17.20",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-plugin": "2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^5.0.9",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "ts-jest": "26.5.1",
    "ttypescript": "1.5.12",
    "typescript": "^4.1.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "always"
  },
  "engines": {
    "node": ">10.0.4"
  },
  "jest": {
    "globalSetup": "./jest-global-setup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "roots": [
      "src/"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
