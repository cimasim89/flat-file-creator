{
  "name": "@codecarvalho/file-positional",
  "private": false,
  "version": "2.0.1",
  "description": "Generate a flat positional text file of your dataset",
  "type": "module",
  "main": "./dist/flat-file-creator.js",
  "types": "./dist/flat-file-creator.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "test": "npx vitest run",
    "watch": "npx vitest",
    "lint": "eslint --cache src/*.ts",
    "lint:fix": "eslint --fix --cache src/*.ts",
    "typecheck": "npx tsc --noEmit",
    "prettier:check": "npx prettier -c src",
    "prettier:fix": "npx prettier -w src",
    "build": "rimraf dist && npx tsc",
    "prepublishOnly": "rimraf dist && npx tsc"
  },
  "keywords": [
    "positional",
    "file-generator",
    "flat-file",
    "positional-dataset"
  ],
  "author": "direisc",
  "license": "MIT",
  "homepage": "https://github.com/direisc/flat-file-creator#readme",
  "repository": "github:direisc/flat-file-creator",
  "bugs": {
    "url": "https://github.com/direisc/flat-file-creator/issues"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
