    "typecheck": "npx tsc --noEmit",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "positional",
    "file-generator",
    "flat-file",
    "positional-dataset"
  ],
  "author": "direisc",
  "license": "MIT",
  "homepage": "https://github.com/direisc/flat-file-creator#readme",
  "repository": "github:direisc/flat-file-creator",
  "bugs": {
    "url": "https://github.com/direisc/flat-file-creator/issues"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "16.7.10",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-eslint-plugin": "3.5.3",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "5.0.0",
    "jest": "27.1.1",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "ts-jest": "27.0.5",
    "ttypescript": "1.5.12",
    "typescript": "4.4.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "always"
  },
  "engines": {
    "node": ">10.0.4"
  },
  "jest": {
    "globalSetup": "./jest-global-setup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "roots": [
      "src/"
    ]
  }
