{
  "name": "flat-file-creator",
  "version": "1.1.0",
  "description": "Generate a flat positional text file of your dataset",
  "main": "./dist/flat-file-creator.js",
  "types": "./dist/flat-file-creator.d.ts",
  "scripts": {
    "test": "jest",
    "watch": "jest --watch *.js",
    "lint": "eslint . --cache --ext .js",
    "lint:fix": "eslint . --cache  --fix --ext .js",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "positional",
    "file-generator",
    "flat-file",
    "positional-dataset"
  ],
  "author": "cimasim89",
  "email": "cimasim89@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/cimasim89/flat-file-creator#readme",
  "repository": "github:cimasim89/flat-file-creator",
  "bugs": {
    "url": "https://github.com/cimasim89/flat-file-creator/issues"
  },
  "dependencies": {
    "@types/lodash": "4.14.151",
    "lodash": "4.17.15",
    "moment": "2.25.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.2.3",
    "@types/node": "10.17.19",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-eslint-plugin": "2.2.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "^4.2.5",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "pretty-quick": "^1.10.0",
    "ts-jest": "^26.0.0",
    "typescript": "3.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "always"
  },
  "engines": {
    "node": ">10.0.4"
  },
  "jest": {
    "globalSetup": "./jest-global-setup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
